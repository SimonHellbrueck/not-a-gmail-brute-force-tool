{
  "manifest_version": 3,
  "name": "Gmail looker",
  "version": "1.0.0",
  "description": "Checks provided Gmail addresses",
  "short_name": "Gmail looker",
  "content_scripts": [
    {
      "js": ["jquery.min.js","mailChecker.js"],
      "matches": ["https://accounts.google.com/servicelogin/*"],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },"permissions": [
  "activeTab",  "storage"
]
}
